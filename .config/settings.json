{
  "go.inlayHints.assignVariableTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.constantValues": true,
  "go.inlayHints.parameterNames": false,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.rangeVariableTypes": true,
  "editor.inlayHints.fontFamily": "JetBrainsMono NF",
  "editor.inlayHints.enabled": "on",
  "editor.inlayHints.padding": true,
  "editor.inlayHints.fontSize": 13,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 10,
  "find-it-faster.general.useTerminalInEditor": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false
  },
  "tailwindCSS.includeLanguages": {
    "templ": "html"
  },
  "editor.indentSize": "tabSize",
  "editor.tabSize": 2,
  "editor.fontSize": 13,
  "terminal.integrated.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "editor.cursorBlinking": "phase",
  "editor.cursorStyle": "line",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.lineNumbers": "relative",
  "git.openRepositoryInParentFolders": "never",
  "window.titleBarStyle": "custom",
  "svelte.enable-ts-plugin": true,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1,
    "asvetliakov.vscode-neovim": 1
  },
  "go.useLanguageServer": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": "always"
  },
  "gopls": {
    "formatting.gofumpt": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    ".obsidian": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "workbench.layoutControl.enabled": false,
  // "window.commandCenter": false,
  "workbench.list.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  // // "editor.guides.bracketPairsHorizontal": false,
  // "editor.guides.highlightActiveIndentation": false,
  "workbench.startupEditor": "welcomePage",
  // "explorer.compactFolders": false,
  "explorer.decorations.badges": true,
  "explorer.decorations.colors": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "*.go": "${capture}_test.go",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "*.templ": "${capture}_templ.go,${capture}_templ.txt"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "entity.name.function",
        "settings": {
          "fontStyle": "underline",
        }
      },
      {
        "scope": "entity.name.function.support",
        "settings": {
          "fontStyle": "",
          "foreground": "#EFB080"
        }
      },
      {
        "scope": "keyword.var",
        "settings": {
          "foreground": "#A99CF5",
        }
      },
      {
        "scope": "keyword.control",
        "settings": {
          "fontStyle": "italic bold",
        }
      },
    ]
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.activityBar.location": "top",
  "go.toolsManagement.autoUpdate": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "workbench.colorTheme": "cursor",
  "editor.fontLigatures": true,
  "editor.fontFamily": "JetBrainsMono NF",
  "terminal.integrated.fontFamily": "JetBrainsMono NF, Monaco, 'Courier New', monospace",
  "workbench.editorAssociations": {
    "{git,gitlens}:/**/*.{md,csv,svg}": "default"
  },
  "workbench.editor.showTabs": "single",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#82D2CE",
    "statusBarItem.remoteBackground": "#82D2CE",
    "editorCursor.foreground": "#82D2CE"
  },
  "nvim-ui.nvimColorNormal": "#82D2CE",
  "nvim-ui.nvimColorInsert": "#EFB080",
  "nvim-ui.nvimColorVisual": "#AB9BF5",
  "nvim-ui.nvimColorReplace": "#000",
  "nvim-ui.nvimColorCustomizationKeys": [
    "tab.activeBorder",
    "statusBarItem.remoteBackground",
    "editorCursor.foreground",
  ],
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.defaultLocation": "editor",
  "terminal.integrated.windowsEnableConpty": false,
}