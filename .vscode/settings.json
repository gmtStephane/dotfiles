{
  "editor.fontFamily": "'JetbrainsMono NF',Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontFamily": "'JetbrainsMono NF',Menlo, Monaco, 'Courier New', monospace",
  "terminal.integrated.fontSize": 13,
  "workbench.activityBar.location": "hidden",
  "workbench.editor.enablePreview": false,
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.overviewRulerBorder": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.guides.indentation": false,
  "editor.cursorBlinking": "phase",
  "editor.showFoldingControls": "never",
  "editor.cursorStyle": "block",
  "editor.cursorSmoothCaretAnimation": "on",
  "debug.terminal.clearBeforeReusing": true,
  "workbench.statusBar.visible": true,
  "redhat.telemetry.enabled": false,
  "breadcrumbs.enabled": false,
  "workbench.tree.renderIndentGuides": "onHover",
  "explorer.decorations.colors": false,
  "workbench.productIconTheme": "material-product-icons",
  "workbench.layoutControl.type": "menu",
  "workbench.layoutControl.enabled": false,
  "go.useLanguageServer": true,
  "go.inlayHints.assignVariableTypes": true,
  "go.inlayHints.compositeLiteralFields": true,
  "go.inlayHints.compositeLiteralTypes": true,
  "go.inlayHints.constantValues": true,
  "go.inlayHints.functionTypeParameters": true,
  "go.inlayHints.parameterNames": true,
  "go.inlayHints.rangeVariableTypes": true,
  "gopls": {
    "formatting.gofumpt": true,
    "ui.semanticTokens": true,
    "ui.completion.usePlaceholders": true
  },
  "terminal.explorerKind": "external",
  "go.lintTool": "golangci-lint",
  "go.buildTags": "integration,playground,functional",
  "go.toolsEnvVars": {
    "GOARCH": "arm64",
    "GOHOSTARCH": "arm64"
  },
  "go.testTimeout": "300s",
  "go.testEnvVars": {
    "SDDE_TEST_PERSISTANT": "integration_test_stephane",
    "CGO_ENABLED": "1",
    "CGO_CFLAGS": "-I/opt/homebrew/Cellar/libxml2/2.11.5_1/include/libxml2",
    "CGO_LDFLAGS": "-L/opt/homebrew/Cellar/libxml2/2.11.5_1/lib -lxml2 -lz -liconv -L/opt/homebrew/Cellar/icu4c/73.2/lib -licui18n -licuuc -licudata"
  },
  "go.testFlags": ["-v"],
  "workbench.iconTheme": "material-icon-theme",
  "gitlens.views.searchAndCompare.files.icon": "status",
  "material-icon-theme.folders.color": "#ff7043",
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "window.commandCenter": false,
  "workbench.colorTheme": "Gitpod Dark",
  "workbench.colorCustomizations": {
    "editorGroupHeader.noTabsBackground": "#12100c",
    "editorGroupHeader.tabsBackground": "#12100c",
    "menu.background": "#12100c",
    "activityBar.background": "#12100c",
    "tab.inactiveBackground": "#12100c",
    "tab.activeBackground": "#a35800",
    "list.inactiveSelectionBackground": "#a35800",
    "sideBar.background": "#12100c",
    "sideBarSectionHeaer.background": "#12100c",
    "statusBar.background": "#12100c",
    "statusBar.noFolderBackground": "#12100c",
    "titleBar.activeBackground": "#12100c",
    "terminal.background": "#12100c",
    "terminal.ansiBrightMagenta": "#a35800",
    "terminal.ansiBrightCyan": "#a35800"
  },
  "window.titleBarStyle": "custom",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.editorActionsLocation": "hidden"
}
